<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="net.catten.hrsys.data">
    <class name="Department" table="DepartmentInfo">
        <id name="id"/>
        <property name="name"/>
        <property name="commit" column="Details"/>
    </class>
</hibernate-mapping>